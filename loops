//loops e condições
for (var i = 0; i < 10; i++){
    console.log("i tá valendo: " + i);
}

a = 3;
while (a > 0){
    console.log("valor de a: " + a);
    a--;
}

b = 4;
switch(b){
    case 1:
    console.log("b é " + b);
    break;
    case 2:
    console.log("b é " + b);
    break;
    case 3:
    console.log("b é " + b);
    break;
    case 4:
    console.log("b é " + b);
    break;
}
